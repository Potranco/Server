{
  "name": "server-express",
  "version": "1.0.0",
  "description": "Server Node.js with Express",
  "main": "dist/server.js",
  "author": "Potranco",
  "license": "ISC",
  "scripts": {
    "start": "npm run compile && node dist/server.js",
    "dev": "gulp dev && concurrently \"gulp watch\" \"nodemon ./dist/server.js\"",
    "compile": "gulp",
    "validate": "standard",
    "validateFix": "standard --fix",
    "test": "npm run validate && npm run compile && mocha --exit"
  },
  "dependencies": {
    "express": "^4.16.2",
    "gulp-exec": "^3.0.1",
    "mongodb": "^3.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^8.0.0",
    "body-parser": "^1.18.2",
    "browserify": "^16.1.1",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-uglify": "^3.0.0",
    "isomorphic-fetch": "^2.2.1",
    "mocha": "^5.0.1",
    "mongoose": "^5.0.11",
    "nodemon": "^1.17.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "standard": "^11.0.0",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "standard": {
    "ignore": [
      "gulpfile.js",
      "/dist/",
      "/test/"
    ]
  }
}
